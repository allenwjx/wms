<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap>

    
    <resultMap id="RM.Freight" class="com.zeh.wms.dal.dataobject.FreightDO">
		<result property="id" column="id" javaType="long" jdbcType="BIGINT"  nullValue="0"  />
		<result property="provinceCode" column="province_code" javaType="java.lang.String" jdbcType="VARCHAR"  />
		<result property="firstWeight" column="first_weight" javaType="int" jdbcType="INTEGER"  nullValue="0"  />
		<result property="firstPrice" column="first_price" javaType="int" jdbcType="INTEGER"  nullValue="0"  />
		<result property="additionalPrice" column="additional_price" javaType="int" jdbcType="INTEGER"  nullValue="0"  />
		<result property="gmtCreate" column="gmt_create" javaType="java.util.Date" jdbcType="TIMESTAMP"  />
		<result property="gmtModified" column="gmt_modified" javaType="java.util.Date" jdbcType="TIMESTAMP"  />
		<result property="createBy" column="create_by" javaType="java.lang.String" jdbcType="VARCHAR"  />
		<result property="modifyBy" column="modify_by" javaType="java.lang.String" jdbcType="VARCHAR"  />
    </resultMap>
    
	<sql id="Freight.columns">
		id, province_code, first_weight, first_price, additional_price, gmt_create, gmt_modified, create_by, modify_by
	</sql>
		

	
	
    
	<insert id="wms.Freight.insert">
		INSERT INTO freight (
	        	id ,province_code ,first_weight ,first_price ,additional_price ,gmt_create ,gmt_modified ,create_by ,modify_by 
	        ) VALUES (#id#,#provinceCode#,#firstWeight#,#firstPrice#,#additionalPrice#,#gmtCreate#,#gmtModified#,#createBy#,#modifyBy#)
		<selectKey resultClass="long" type="post" keyProperty="id" >
            select last_insert_id()
    	</selectKey>        
	</insert>
	
	
	<delete id="wms.Freight.delete">
		DELETE FROM freight WHERE
	        id = #id#
    </delete>
    
	
	<update id="wms.Freight.update">
		UPDATE freight SET
		        province_code = #provinceCode# ,first_weight = #firstWeight# ,first_price = #firstPrice# ,additional_price = #additionalPrice# ,gmt_create = #gmtCreate# ,gmt_modified = #gmtModified# ,create_by = #createBy# ,modify_by = #modifyBy# 
	        WHERE 
	        	id = #id#
	</update>
	
    
	
	<select id="wms.Freight.queryById" resultMap="RM.Freight" >
    	SELECT
 /*wms.Freight.queryById*/ 		    <include refid="Freight.columns"/>
	        FROM freight 
	        WHERE 
		        id = #id#
	</select>	

	    
	
	
    
	
	<select id="wms.Freight.findPage" resultMap="RM.Freight" >
    	SELECT  /*wms.Freight.findPage*/ 
			<include refid="Freight.columns"/>
			FROM freight
			limit #offset#,#limit#
	</select>	

	<select id="wms.Freight.findPage.count" resultClass="long" >
    	select  /*wms.Freight.findPage.count*/ count(*) 	FROM freight
	</select>
	    
	
	
    

</sqlMap>

