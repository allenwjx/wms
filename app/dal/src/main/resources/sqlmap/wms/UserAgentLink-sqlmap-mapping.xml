		<selectKey resultClass="long" type="post" keyProperty="id" >
            select last_insert_id()
    	</selectKey>        
	</insert>
	
	
	<delete id="wms.UserAgentLink.delete">
		DELETE FROM user_agent_link WHERE
	        id = #id#
    </delete>
    
	
	<update id="wms.UserAgentLink.update">
		UPDATE user_agent_link SET
		        user_id = #userId# ,code = #code# ,type = #type# ,link_status = #linkStatus# ,gmt_create = #gmtCreate# ,gmt_modified = #gmtModified# ,create_by = #createBy# ,modify_by = #modifyBy# 
