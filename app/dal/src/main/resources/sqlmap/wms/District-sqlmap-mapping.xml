<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap>

    
    <resultMap id="RM.District" class="com.zeh.wms.dal.dataobject.DistrictDO">
		<result property="id" column="id" javaType="long" jdbcType="BIGINT"  nullValue="0"  />
		<result property="code" column="code" javaType="java.lang.String" jdbcType="VARCHAR"  />
		<result property="name" column="name" javaType="java.lang.String" jdbcType="VARCHAR"  />
		<result property="gmtCreate" column="gmt_create" javaType="java.util.Date" jdbcType="TIMESTAMP"  />
		<result property="gmtModified" column="gmt_modified" javaType="java.util.Date" jdbcType="TIMESTAMP"  />
		<result property="createBy" column="create_by" javaType="java.lang.String" jdbcType="VARCHAR"  />
		<result property="modifyBy" column="modify_by" javaType="java.lang.String" jdbcType="VARCHAR"  />
    </resultMap>
    
	<sql id="District.columns">
		id, code, name, gmt_create, gmt_modified, create_by, modify_by
	</sql>
		

	
	
    
	<insert id="wms.District.insert">
		INSERT INTO district (
	        	id ,code ,name ,gmt_create ,gmt_modified ,create_by ,modify_by 
	        ) VALUES (#id#,#code#,#name#,#gmtCreate#,#gmtModified#,#createBy#,#modifyBy#)
		<selectKey resultClass="long" type="post" keyProperty="id" >
            select last_insert_id()
    	</selectKey>        
	</insert>
	
	
	<delete id="wms.District.delete">
		DELETE FROM district WHERE
	        id = #id#
    </delete>
    
	
	<update id="wms.District.update">
		UPDATE district SET
		        code = #code# ,name = #name# ,gmt_create = #gmtCreate# ,gmt_modified = #gmtModified# ,create_by = #createBy# ,modify_by = #modifyBy# 
	        WHERE 
	        	id = #id#
	</update>
	
    
	
	<select id="wms.District.queryById" resultMap="RM.District" >
    	SELECT
 /*wms.District.queryById*/ 		    <include refid="District.columns"/>
	        FROM district 
	        WHERE 
		        id = #id#
	</select>	

	    
	
	
    
	
	<select id="wms.District.findPage" resultMap="RM.District" >
    	SELECT  /*wms.District.findPage*/ 
			<include refid="District.columns"/>
			FROM district
			limit #offset#,#limit#
	</select>	

	<select id="wms.District.findPage.count" resultClass="long" >
    	select  /*wms.District.findPage.count*/ count(*) 	FROM district
	</select>
	    
	
	
    

</sqlMap>

