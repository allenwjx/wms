#set($layout="layout/spa.vm")
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <style>
        body {
            padding-top: 60px;
        }
    </style>
    <link rel="stylesheet" type="text/css" href="${rc.contextPath}/js/plugins/bootstrap/css/bootstrap.min.css"/>
    <link rel="stylesheet" type="text/css" href="${rc.contextPath}/js/plugins/font-awesome/css/font-awesome.min.css"/>
    <link rel="stylesheet" type="text/css" href="${rc.contextPath}/js/plugins/bootstrap-toastr/toastr.min.css"/>
    <link rel="stylesheet" type="text/css" href="${rc.contextPath}/js/plugins/bootstrap-validator/bootstrapValidator.min.css"/>
    <link rel="stylesheet" type="text/css" href="${rc.contextPath}/css/login-register.css"/>
    <script src="${rc.contextPath}/js/plugins/jquery-1.12.2.min.js" type="text/javascript"></script>
    <script src="${rc.contextPath}/js/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
</head>
<body>
<div class="container">
    <div class="modal-dialog login animated">
        <div id="login" class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Login with</h4>
            </div>
            <div class="modal-body">
                <div class="box">
                    <form id="loginForm">
                        <div class="content">
                            <div class="error"></div>
                            <div class="form loginBox">
                                <div class="form-group">
                                    <input id="username" class="form-control" type="text" placeholder="账号" name="username" v-model.trim="model.username">
                                </div>
                                <div class="form-group">
                                    <input id="password" class="form-control" type="password" placeholder="密码" name="password" v-model.trim="model.password">
                                </div>
                                <div class="form-group">
                                    <input class="btn btn-default btn-login" type="button" value="登录" v-on:click="login">
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
</body>
</html>
<script src="${rc.contextPath}/js/wms/login/login.js" type="text/javascript"></script>
<script type="text/javascript" src="${rc.contextPath}/js/plugins/vue.js"></script>
<script type="text/javascript" src="${rc.contextPath}/js/plugins/bootstrap-validator/bootstrapValidator.min.js"></script>
<script src="${rc.contextPath}/js/plugins/bootstrap-toastr/toastr.min.js"></script>
