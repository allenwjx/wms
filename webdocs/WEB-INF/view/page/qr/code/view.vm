#set($layout="layout/spa.vm")
## Title
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    <h4 class="modal-title" id="myModalLabel">绑定商品维护</h4>
</div>

## Form
<form id="modalForm" class="form-horizontal">
    <div class="modal-body container-fluid ">
        <div class="form-group">
            <label class="col-sm-3 control-label" for="type">二维码ID</label>
            <div class="col-sm-6">
                <input class="form-control" id="id" name="id" type="text" v-model.trim="model.id" :disabled="true"/>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label" for="type">商品ID</label>
            <div class="col-sm-6">
                <input class="form-control" id="commodityId" name="commodityId" type="text" v-model.trim="model.commodityId"/>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button id="dialog-submit" type="button" @click.prevent="submitData" class="btn blue">绑定</button>
        <button id="dialog-cancel" type="button" class="btn btn-warning" data-dismiss="modal">取消</button>
    </div>
</form>
<script type="text/javascript">
    var formData = $!{modelData};
</script>
<script type="text/javascript" src="${rc.contextPath}/js/wms/qr/code/view.js"></script>