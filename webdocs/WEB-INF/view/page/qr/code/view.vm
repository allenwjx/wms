#set($layout="layout/spa.vm")
<div id="qrcode_bind">
## Title
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
        <h4 class="modal-title" id="myModalLabel"><span aria-hidden="true" class="icon-plane"></span>二维码 {{model.serialNo}} 绑定商品</h4>
    </div>

## Form
    <form class="form-horizontal">
        <input type="hidden" id="objId" value="$!{id}">
        <div class="modal-body container-fluid ">
            <div class="row">
                <div class="col-md-6 col-sm-12">
                    <div class="portlet green-meadow box">
                        <div class="portlet-title">
                            <div class="caption">
                                <i class="fa fa-cogs"></i>待绑定商品
                            </div>
                            <div class="tools">
                                <a href="javascript:;" class="collapse">
                                </a>
                                <a href="javascript:;" class="reload">
                                </a>
                            </div>
                        </div>
                        <div class="portlet-body">
                            <div class="row static-info">
                                <div class="col-md-5 name">
                                    ID:
                                </div>
                                <div class="col-md-7 value">
                                    {{ model.commodityId }} <span class="label label-info label-sm"> 商品已绑定 </span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <span aria-hidden="true">&nbsp;</span>
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button id="dialog-cancel" type="button" class="btn btn-warning" data-dismiss="modal">关闭</button>
        </div>
    </form>
</div>
<script type="text/javascript" src="${rc.contextPath}/js/wms/qr/code/view.js"></script>