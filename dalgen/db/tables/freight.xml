<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table SYSTEM "http://sources.jungle.net/svn/dtd/table-config-1.0.dtd">

<table sqlname="freight">
	<!-- 不需要配置的列请删除 -->
	
	<sql id="Freight.columns">
		id, province_code, first_weight, first_price, additional_price, gmt_create, gmt_modified, create_by, modify_by 
	</sql>
	
	<operation name="insert" paramtype="object">
		<sql>
			INSERT INTO freight (
	        	id ,province_code ,first_weight ,first_price ,additional_price ,gmt_create ,gmt_modified ,create_by ,modify_by 
	        ) VALUES (
	        	? ,? ,? ,? ,? ,? ,? ,? ,?         
	        )
		</sql>		
	</operation>

	<operation name="delete">
		<sql>
	        DELETE FROM freight WHERE
	        id = ? 
		</sql>
	</operation>
	
	<operation name="update" paramtype="object">
		<sql>
	        UPDATE freight SET
		        province_code = ? ,first_weight = ? ,first_price = ? ,additional_price = ? ,gmt_create = ? ,gmt_modified = ? ,create_by = ? ,modify_by = ? 
	        WHERE 
	        	id = ? 
		</sql>					
	</operation>

	<operation name="queryById" multiplicity="one">
		<sql>
	    	SELECT
		    <include refid="Freight.columns"/>
	        FROM freight 
	        WHERE 
		        id = ? 
		</sql>
	</operation>

	
	<operation name="findPage" multiplicity="paging">
		<sql>
			SELECT 
			<include refid="Freight.columns"/>
			FROM freight 
		</sql>
	</operation>

</table>
